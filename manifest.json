{
    "manifest_version": 3,
    "name": "YouTube Token Extension",
    "version": "1.0",
    "description": "An extension to use a token for YouTube.",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  